{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "max-len": "off",
    "object-curly-newline": "off",
    "no-param-reassign": "off"
  }
}